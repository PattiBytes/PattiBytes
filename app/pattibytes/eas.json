{
  "cli": {
    "version": ">= 18.0.5",
    "appVersionSource": "remote",
    "requireCommit": false
  },
  "build": {
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EAS_PROJECT_ROOT": "app/pattibytes",
        "APP_VARIANT": "preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EAS_PROJECT_ROOT": "app/pattibytes"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
