{
  "cli": {
    "version": ">= 18.0.5",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "prebuildCommand": "cd app/pattibytes && npx expo prebuild"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "prebuildCommand": "cd app/pattibytes && npx expo prebuild"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "prebuildCommand": "cd app/pattibytes && npx expo prebuild"
    }
  },
  "submit": {
    "production": {}
  }
}
